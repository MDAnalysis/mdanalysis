              %other-table-wrap-atts;
              %display-atts;"                                        >

                                                                
<!--                    TABLE WRAPPER ATTRIBUTES                   -->
<!--                    Attributes for Table Wrapper <table-wrap>  -->
<!ENTITY % table-wrap-atts
             "id        ID                                #IMPLIED
              %display-atts;                                       
